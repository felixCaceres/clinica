     ficha_medica.`efpa` AS ficha_medica_efpa,
     ficha_medica.`efsat` AS ficha_medica_efsat,
     ficha_medica.`effc` AS ficha_medica_effc,
     ficha_medica.`eftemp` AS ficha_medica_eftemp,
     ficha_medica.`estudiosolicitado` AS ficha_medica_estudiosolicitado,
     ficha_medica.`imprediagnostico` AS ficha_medica_imprediagnostico,
     ficha_medica.`tratamiento` AS ficha_medica_tratamiento,
     paciente_A.`id` AS paciente_A_id,
     paciente_A.`documento` AS paciente_A_documento,
     paciente_A.`nombre` AS paciente_A_nombre,
     paciente_A.`apellido` AS paciente_A_apellido,
     paciente_A.`fechanac` AS paciente_A_fechanac,
     paciente_A.`tel` AS paciente_A_tel,
     paciente_A.`lugar_consulta` AS paciente_A_lugar_consulta,
     paciente_A.`cel` AS paciente_A_cel,
     seguimiento.`id` AS seguimiento_id,
     seguimiento.`paciente_id` AS seguimiento_paciente_id,
     seguimiento.`seguimiento` AS seguimiento_seguimiento
FROM
     `paciente` paciente INNER JOIN `ficha_medica` ficha_medica ON paciente.`id` = ficha_medica.`paciente_id`
     INNER JOIN `paciente` paciente_A ON ficha_medica.`paciente_id` = paciente_A.`id`
